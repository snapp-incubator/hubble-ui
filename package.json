    "pluralize": "8.0.0",
    "prop-types": "15.8.1",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.1",
    "react-spring": "9.7.3",
    "react-window": "1.8.10",
